{
  "input_directory": "./assets/input/videos",
  "output_directory": "./assets/output/videos",
  "model": {
    "type": "YOLO",
    "weights": "yolo11m.pt"
  },
  "video_extensions": [".mp4", ".avi", ".mov", ".mkv", ".wmv", ".flv"],
  "inference_parameters": {
    "conf": 0.3,
    "iou": 0.45,
    "classes": [0],
    "verbose": false
  },
  "video_dimensions": {
    "width": 1280,
    "height": 720
  }
}
